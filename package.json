{
  "name": "rmf-market-pulse-mcp",
  "version": "1.0.0",
  "description": "MCP server providing Thai Retirement Mutual Fund (RMF) market data via Model Context Protocol",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "thai-rmf",
    "mutual-funds",
    "retirement-funds",
    "thailand",
    "sec-api"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tkhongsap/rmf-market-pulse-mcp"
  },
  "scripts": {
    "dev": "tsx server/index.ts",
    "start": "node dist/index.js",
    "build": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "check": "tsc --noEmit",
    "test": "tsx test-mcp-tools.ts",
    "test:coverage": "c8 --reporter=text --reporter=lcov --include='server/**/*.ts' --exclude='server/index.ts' tsx test-mcp-tools.ts",
    "test:http": "./test-mcp-http.sh",
    "data:rmf:generate-list": "tsx scripts/data-extraction/rmf/generate-rmf-fund-list.ts",
    "data:rmf:build-mapping": "tsx scripts/data-extraction/rmf/phase-0-build-mapping.ts",
    "data:rmf:fetch-all": "tsx scripts/data-extraction/rmf/phase-1-fetch-all-funds.ts",
    "data:rmf:identify-incomplete": "tsx scripts/data-extraction/rmf/identify-incomplete-funds.ts",
    "data:rmf:reprocess": "tsx scripts/data-extraction/rmf/reprocess-incomplete-funds.ts",
    "data:rmf:parse": "node scripts/data-parsing/rmf/parse-rmf-funds.js",
    "data:rmf:consolidate-csv": "tsx scripts/data-extraction/rmf/consolidate-to-csv.ts",
    "data:esg:generate-list": "tsx scripts/data-extraction/esg/generate-thai-esg-fund-list.ts",
    "data:esgx:generate-list": "tsx scripts/data-extraction/esgx/generate-thai-esgx-fund-list.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.21.1",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/node": "20.16.11",
    "c8": "^10.1.3",
    "esbuild": "^0.25.0",
    "tsx": "^4.20.5",
    "typescript": "5.6.3"
  }
}
