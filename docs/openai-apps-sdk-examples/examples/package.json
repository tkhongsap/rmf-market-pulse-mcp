{
  "name": "rmf-mcp-openai-apps-sdk",
  "version": "1.0.0",
  "description": "RMF Market Pulse MCP Server with OpenAI Apps SDK widget support",
  "type": "module",
  "scripts": {
    "dev": "vite --config vite.config.ts",
    "build": "tsx build-all.ts",
    "serve": "serve -s ./assets -p 4444 --cors",
    "server:node": "cd server-node && tsx src/server.ts",
    "server:python": "cd server-python && python main.py",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "esbuild": "^0.19.11",
    "eslint": "^8.56.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "serve": "^14.2.1",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "openai",
    "apps-sdk",
    "chatgpt",
    "rmf",
    "thailand",
    "mutual-funds"
  ],
  "author": "",
  "license": "MIT"
}
